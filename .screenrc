autodetach on
bell_msg "^G"
defscrollback 10000
escape ^Zz
vbell off
bind h prev
bind j next
bind l windowlist

hardstatus alwayslastline "%{=}%{Kw}[%m/%d %02c]%{Kc} %-w%{y}%n %t%{-}%+w %=%{kC} [ %H ] "

#multiuser on
##acladd tak
startup_message off
# <C-k> kill
bind k  kill
bind s 
bind ^z meta

#bind s colon "at 0 exec ssh "
bind t colon 'title '
bind n colon 'sessionname '

bind r eval 'echo "Resize window"' 'command -c resize'
bind -c resize ^]  command
bind -c resize j eval 'resize +1' 'command -c resize'
bind -c resize k eval 'resize -1' 'command -c resize'


defutf8 on
defencoding utf8
encoding utf-8 utf-8
cjkwidth on
termcapinfo kterm 'cl=\E[H\E[2J'
setenv SCREEN true

